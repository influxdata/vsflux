{
  "name": "flux",
  "displayName": "Flux for VSCode",
  "description": "Flux language extension for VSCode",
  "version": "0.0.1",
  "icon": "images/icon.png",
  "author": {
    "name": "InfluxData"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/influxdata/vsflux"
  },
  "bugs": {
    "url": "https://github.com/influxdata/vsflux/issues"
  },
  "engines": {
    "vscode": "^1.39.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "activationEvents": [
    "onLanguage:flux"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "flux",
        "aliases": [
          "Flux"
        ],
        "extensions": [
          ".flux"
        ]
      }
    ],
    "grammars": [
      {
        "language": "flux",
        "scopeName": "flux",
        "path": "./syntaxes/flux.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "flux",
        "path": "./snippets/flux.json"
      }
    ],
    "configuration": {
      "title": "flux configuration",
      "properties": {}
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.39.0",
    "@types/keytar": "^4.0.1",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "ts-loader": "^6.2.1",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1",
    "vscode-test": "^1.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "vscode-languageclient": "^5.2.1"
  }
}
